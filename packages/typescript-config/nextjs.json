{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",

    "noEmit": true,

    "allowJs": true,

    "plugins": [{ "name": "next" }],

    "jsx": "preserve"
  }
}
